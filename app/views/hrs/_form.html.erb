<%= form_with(model: hr, local: true) do |form| %>
  <div >
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div> 
  <div >
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div >
    <%= form.label :status %>
    <%= form.text_field :status %>
  </div>
  <div >
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div >
    <%= form.label :phone_number %>
    <%= form.telephone_field :phone_number %>
  </div>
  <div >
    <%= form.submit %>
  </div>
<% end %>

